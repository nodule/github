{
  "name": "getSha",
  "ns": "github",
  "description": "Retrieve Sha for file",
  "phrases": {
    "active": "retrieving Sha for path {{input.path}}"
  },
  "ports": {
    "input": {
      "repo": {
        "title": "Repository",
        "type": "function",
        "readonly": true
      },
      "path": {
        "title": "Path",
        "type": "string"
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "sha": {
        "title": "Sha",
        "type": "string"
      }
    }
  }
}
