{
  "name": "write",
  "ns": "github",
  "description": "Write File",
  "phrases": {
    "active": "Writing {{input.path}} to the {{input.branch}} branch. [{{input.commit_message}}]"
  },
  "ports": {
    "input": {
      "repo": {
        "title": "Repository",
        "type": "function"
      },
      "branch": {
        "title": "Branch",
        "type": "string",
        "default": "master"
      },
      "path": {
        "title": "Path",
        "type": "string"
      },
      "contents": {
        "title": "Contents",
        "type": "string"
      },
      "commit_message": {
        "title": "Commit message",
        "type": "string"
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      }
    }
  }
}
