{
  "name": "read",
  "ns": "github",
  "async": true,
  "description": "Read path",
  "phrases": {
    "active": "Reading {{input.path}} from the {{input.branch}} branch"
  },
  "ports": {
    "input": {
      "repo": {
        "title": "Repository",
        "type": "function"
      },
      "branch": {
        "title": "Branch",
        "type": "string",
        "default": "master"
      },
      "path": {
        "title": "Path",
        "type": "string",
        "async": true
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "content": {
        "title": "Content",
        "type": "object"
      },
      "sha": {
        "title": "SHA",
        "type": "object"
      }
    }
  }
}