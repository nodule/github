{
  "name": "github",
  "description": "Github for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/github.git"
  },
  "dependencies": {
    "github-api": "0.x.x"
  },
  "nodes": [
    {
      "name": "api",
      "ns": "github",
      "description": "Github API",
      "phrases": {
        "active": "Authenticating with github api"
      },
      "require": {
        "github-api": "0.x.x"
      },
      "ports": {
        "input": {
          "oauth_token": {
            "title": "OAUTH Token",
            "type": "string"
          }
        },
        "output": {
          "github": {
            "title": "Github",
            "type": "function"
          }
        }
      },
      "fn": "output = {\n  github: new github_api( {\n    token: input.oauth_token\n  })\n}\n"
    },
    {
      "name": "getRepo",
      "ns": "github",
      "description": "Get Repository",
      "phrases": {
        "active": "getting repository {{input.reponame}}"
      },
      "ports": {
        "input": {
          "github": {
            "title": "Github",
            "type": "function",
            "readonly": true
          },
          "username": {
            "title": "Username",
            "type": "string"
          },
          "reponame": {
            "title": "Repository name",
            "type": "string"
          }
        },
        "output": {
          "repo": {
            "title": "Repository",
            "type": "function"
          }
        }
      },
      "fn": "output = {\n repo: input.github.getRepo(input.username, input.reponame)\n}\n"
    },
    {
      "name": "getSha",
      "ns": "github",
      "description": "Retrieve Sha for file",
      "phrases": {
        "active": "retrieving Sha for path {{input.path}}"
      },
      "ports": {
        "input": {
          "repo": {
            "title": "Repository",
            "type": "function",
            "readonly": true
          },
          "path": {
            "title": "Path",
            "type": "string"
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "sha": {
            "title": "Sha",
            "type": "string"
          }
        }
      },
      "fn": "output = [\n  input.repo,\n  'getSha',\n  input.path\n]\n"
    },
    {
      "name": "getTree",
      "ns": "github",
      "description": "Retrieve repo tree",
      "phrases": {
        "active": "retrieving {{input.branch}} tree"
      },
      "ports": {
        "input": {
          "repo": {
            "title": "Repository",
            "type": "function",
            "readonly": true
          },
          "branch": {
            "title": "Branch",
            "type": "string",
            "default": "master"
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "tree": {
            "title": "Tree",
            "type": "object"
          }
        }
      },
      "fn": "output = [\n  input.repo,\n  'getTree',\n  input.branch\n]\n"
    },
    {
      "name": "login",
      "ns": "github",
      "description": "Github Login",
      "phrases": {
        "active": "Logging into Github"
      },
      "require": {
        "github-api": "0.x.x"
      },
      "ports": {
        "input": {
          "username": {
            "title": "Username",
            "type": "string"
          },
          "password": {
            "title": "Password",
            "type": "password"
          }
        },
        "output": {
          "github": {
            "title": "Github",
            "type": "function"
          }
        }
      },
      "fn": "output = {\n  github: new github_api( {\n    username: input.username,\n    password: input.password\n  })\n}\n"
    },
    {
      "name": "read",
      "ns": "github",
      "description": "Read File",
      "phrases": {
        "active": "Reading {{input.path}} from the {{input.branch}} branch"
      },
      "ports": {
        "input": {
          "repo": {
            "title": "Repository",
            "type": "function",
            "readonly": true
          },
          "branch": {
            "title": "Branch",
            "type": "string",
            "default": "master"
          },
          "path": {
            "title": "Path",
            "type": "string"
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "content": {
            "title": "Content",
            "type": "object"
          },
          "sha": {
            "title": "SHA",
            "type": "object"
          }
        }
      },
      "fn": "output = [\n  input.repo,\n  'read',\n  input.branch,\n  input.path\n]\n"
    },
    {
      "name": "show",
      "ns": "github",
      "description": "Show Repository",
      "phrases": {
        "active": "Reading {{input.path}} from the {{input.branch}} branch"
      },
      "ports": {
        "input": {
          "repo": {
            "title": "Repository",
            "type": "function",
            "readonly": true
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          },
          "repo": {
            "title": "Repository",
            "type": "object"
          }
        }
      },
      "fn": "output = [input.repo, 'show']\n"
    },
    {
      "name": "write",
      "ns": "github",
      "description": "Write File",
      "phrases": {
        "active": "Writing {{input.path}} to the {{input.branch}} branch. [{{input.commit_message}}]"
      },
      "ports": {
        "input": {
          "repo": {
            "title": "Repository",
            "type": "function",
            "readonly": true
          },
          "branch": {
            "title": "Branch",
            "type": "string",
            "default": "master"
          },
          "path": {
            "title": "Path",
            "type": "string"
          },
          "contents": {
            "title": "Contents",
            "type": "string"
          },
          "commit_message": {
            "title": "Commit message",
            "type": "string"
          }
        },
        "output": {
          "error": {
            "title": "Error",
            "type": "object"
          }
        }
      },
      "fn": "output = [\n  input.repo,\n  'write',\n  input.branch,\n  input.path,\n  input.contents,\n  input.commit_message\n]\n"
    }
  ],
